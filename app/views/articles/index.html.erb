<h1>Articles</h1>
<ul>
  <% @articles.each do |article| %>
    <li>
    <%= render "article", article: article %>
    <%=link_to "Show", article%>
    <%=link_to "Edit", article%>
    <%=button_to "Delete", article_path(article.id), method: :delete %>
    </li>
  <% end %>
</ul>
<%=link_to "Create New Artictle", new_article_path %>
